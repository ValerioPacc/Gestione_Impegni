<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
     <Dialog id="listaPNI" title="Lista PNI - Autorizzazioni" draggable="true" showHeader="true" titleAlignment="Center"
      afterClose="onAfterClose" contentWidth="80%" contentHeight="80%">
      
      
                <content>
                <HBox>
                <Label text="Struttura Finanziaria:" vAlign="Inherit"/>
               <Input text="{}" editable="fale"/>
               <Label text="Struttura Amministrativa Responsabile:" vAlign="Inherit"/>
                <Input text="{}" editable="fale"/>
                <Label text="PNI:" required="true" vAlign="Inherit"/>
               <ComboBox value="" showValueHelp=""/>
                </HBox>
                <VBox>
                <CheckBox id="Au1" text="Autorizzazione:" selected="true"/>
                <CheckBox id="Au2" text="Autorizzazione:" selected="true"/>
                </VBox>
                </content>
                
                
      <beginButton>
       <Button text="Indietro" type="Emphasized" press="onCloseDialog"/>
      </beginButton>
      <endButton>
        <Button text="Conferma" type="Emphasized" press=""/>
      </endButton>
   </Dialog>
</core:FragmentDefinition>