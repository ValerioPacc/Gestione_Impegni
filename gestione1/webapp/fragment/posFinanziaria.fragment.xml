<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">>
	<Dialog id="Anagrafica"  draggable="true" showHeader="true" titleAlignment="Start"
      afterClose="onAfterClose" contentWidth="auto" contentHeight="80%">
      <f:Form editable="false" title="Dati Anagrafica Bancario">
         <f:layout>
            <f:ColumnLayout columnsM="1" columnsL="1" columnsXL="1"/>
         </f:layout>
         <f:formContainers>
            <f:FormContainer>
               <f:FormElement visible="{dialogModel>/GgVisible}">
                  <f:label>
                     <Label text="Paese" required= "true"/>
                  </f:label>
                  <f:fields>
                     <MultiInput id="inputPaese" class="validateFilter" value="{}" width="10%">
                     <core:Item key="{comboBox>}" text="{comboBox>}" />
                     </MultiInput>
                      <Input id="input" class="validateFilter" value="{}"></Input>
                  </f:fields>
               </f:FormElement>
               <f:FormElement>
                        <f:label>
                            <Label text="Cat.Beneficiario" required= "true"/>
                        </f:label>
                        <f:fields>
                        <Select selectedKey="" items="{>/}">
                        <core:Item key="{}" text="{}" />
                        </Select>
                        </f:fields>
                </f:FormElement>
                <f:FormElement visible="{dialogModel>/GgVisible}">
                  <f:label>
                     <Label text="Documentazione Beneficiario" required= "true"/>
                  </f:label>
                  <f:fields>
                     <Input id="docBen" class="validateFilter" value="{}">
                     </Input>
                  </f:fields>
               </f:FormElement>
               <f:FormElement visible="{dialogModel>/GgVisible}">
                  <f:label>
                     <Label text="Nome" required= "true"/>
                  </f:label>
                  <f:fields>
                     <Input id="inputNome" class="validateFilter" value="{}">
                     </Input>
                  </f:fields>
               </f:FormElement>
               <f:FormElement visible="{dialogModel>/GgVisible}">
                  <f:label>
                     <Label text="Cognome" required= "true"/>
                  </f:label>
                  <f:fields>
                     <Input id="inputCog" class="validateFilter" value="{}">
                     </Input>
                  </f:fields>
               </f:FormElement>
               <f:FormElement visible="{dialogModel>/GgVisible}">
                  <f:label>
                     <Label text="Via" required= "true"/>
                  </f:label>
                  <f:fields>
                     <Input id="inputVia" class="validateFilter" value="{}">
                     </Input>
                      <Input class="validateFilter" value="{}" width="30%">
                     </Input>
                  </f:fields>
               </f:FormElement>
                <f:FormElement visible="{dialogModel>/GgVisible}">
                  <f:label>
                     <Label text="LocalitÃ " required= "true" labelFor="inputLoc"/>
                    
                  </f:label>
                  <f:fields>
                     <Input id="inputLoc" class="validateFilter" value="{}" width="120%">
                     </Input>
                     <Input id="inputReg1" class="validateFilter" value="{}" >
                     </Input>
                     </f:fields>
                     <Input class="validateFilter" value="{}" width="120%" >
                     </Input>
                  
               </f:FormElement>
                 <f:FormElement visible="{dialogModel>/GgVisible}">
                  <f:label>
                     <Label text="CAP" required= "true"/>
                  </f:label>
                  <f:fields>
                     <Input id="inputCAP" class="validateFilter" value="{}">
                     </Input>
                  </f:fields>
               </f:FormElement>
  </f:FormContainer>
         </f:formContainers>
      </f:Form>
               <f:Form editable="false" title="Dati Fiscali Beneficiario ">
         <f:layout>
            <f:ColumnLayout columnsM="1" columnsL="1" columnsXL="1"/>
         </f:layout>
         <f:formContainers>
            <f:FormContainer>
                <f:FormElement>
                        <f:label>
                            <Label text="Codice Fiscale" required= "true"/>
                        </f:label>
                        <f:fields>
                        <Input id="inputCF" class="validateFilter" value="{}" >
                     </Input>
                        </f:fields>
                </f:FormElement>
                <f:FormElement>
                        <f:label>
                            <Label text="Partita IVA" />
                        </f:label>
                        <f:fields>
                        <Input id="inputPiva" class="validateFilter" value="{}" >
                     </Input>
                        </f:fields>
                </f:FormElement>
                <f:FormElement>
                        <f:label>
                            <Label text="Codice Fiscale/Iva" />
                        </f:label>
                        <f:fields>
                        <Input id="inputCfPiva" class="validateFilter" value="{}" >
                     </Input>
                        </f:fields>
                </f:FormElement>
                
            </f:FormContainer>
         </f:formContainers>
      </f:Form>
      <beginButton>
       <Button text="Indietro" press="onCloseDialog"/>
      </beginButton>
      <endButton>
        <Button text="Salva" type="Emphasized" press="onOpenFragmentBusyDialog"/>
      </endButton>
   </Dialog>
</core:FragmentDefinition>